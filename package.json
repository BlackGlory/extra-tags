    "husky": "^4.3.8",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.4.0",
    "standard-version": "^9.5.0",
    "ts-patch": "^3.2.1",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.12.2",
    "typescript-transform-paths": "^3.5.1",
    "vite": "^5.4.10",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.4.0",
    "extra-utils": "^5.16.0"
  }
