{
  "compilerOptions": {
    "target": "ES2018"
  , "module": "CommonJS"
  , "strict": true
  , "noUnusedLocals": true
  , "noUnusedParameters": true
  , "baseUrl": "."
  , "paths": {
      "@src/*": ["src/*"]
    , "@reducers/*": ["src/reducers/*"]
    , "@converters/*": ["src/converters/*"]
    , "@creators/*": ["src/creators/*"]
    , "@transformers/*": ["src/transformers/*"]
    , "@utils/*": ["src/utils/*"]
    , "@test/*": ["__tests__/*"]
    }
  }
}
